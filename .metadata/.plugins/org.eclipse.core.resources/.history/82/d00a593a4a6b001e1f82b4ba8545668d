#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main()
{


char buff[5];
int flag_pass=0;
printf("\n Enter the password");
gets(buff);
if(strcmp(buff,"2022"))
	printf("\n Bad password");
else
{
	printf("\n correct password");
	flag_pass = 1;
}

if(flag_pass)
{
	printf("\n You are welcome :");

}


	return 0;
}

