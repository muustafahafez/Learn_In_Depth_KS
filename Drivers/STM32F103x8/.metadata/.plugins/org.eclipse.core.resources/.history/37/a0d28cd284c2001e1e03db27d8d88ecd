

#include "GPIO.h"


uint8_t Get_u8CRLH_Position(uint16_t PinNum)
{
	uint8_t Pos;
	switch(PinNum)
	{
	case PIN_NUM_0  : Pos = 2  ; break;
	case PIN_NUM_1  : Pos = 6  ; break;
	case PIN_NUM_2  : Pos = 10 ; break;
	case PIN_NUM_3  : Pos = 14 ; break;
	case PIN_NUM_4  : Pos = 18 ; break;
	case PIN_NUM_5  : Pos = 22 ; break;
	case PIN_NUM_6  : Pos = 26 ; break;
	case PIN_NUM_7  : Pos = 30 ; break;
	case PIN_NUM_8  : Pos = 2  ; break;
	case PIN_NUM_9  : Pos = 6  ; break;
	case PIN_NUM_10 : Pos = 10 ; break;
	case PIN_NUM_11 : Pos = 14 ; break;
	case PIN_NUM_12 : Pos = 18 ; break;
	case PIN_NUM_13 : Pos = 22 ; break;
	case PIN_NUM_14 : Pos = 26 ; break;
	default			: Pos = 30 ; break;

	}
}
