void task1()
{
	static int Count = 0 ;
	while (1){
		Task1LED ^= 1;
//				MYRTOS_TaskWait(100, &Task1);
				Count++ ;
				if (Count == 200 )
				{
//					MYRTOS_AcquireMutex(&Mutex1, &Task1);
					MYRTOS_ActivateTask(&Task2);
				}
//				if (Count == 200 )
//				{
//					Count = 0 ;
////					MYRTOS_ReleaseMutex(&Mutex1);
//					MYRTOS_TerminateTask(&Task1);
//				}
	}
}
void task2()
{
	static int Count = 0 ;
	while (1){
		Task2LED ^= 1;
//				Count++ ;
//				if (Count == 100 )
//				{
//					MYRTOS_ActivateTask(&Task3);
//				}
				if (Count == 200 )
				{
					Count = 0 ;
					MYRTOS_ActivateTask(&Task1);
//					MYRTOS_TerminateTask(&Task2);

				}
	}
}
