#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main()
{


	int x[5]= {1,2,3,4,5};
	x[-2]=10;

for(int i =-20;i<25;i++){
	printf("%d\n",x[i]);

}


	return 0;
}

